{
    "env": {
        "mocha": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-console": "off",
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-restricted-properties": "off"
    }
}
